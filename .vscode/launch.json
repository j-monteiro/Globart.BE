{
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Launch",
                "type": "node",
                "request": "launch",
                "program": "${workspaceRoot}/src/main.js",
                "stopOnEntry": false,
                "args": [],
                "cwd": "${workspaceRoot}",
                "preLaunchTask": null,
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
                "runtimeArgs": [
                    "--nolazy"
                ],
                "env": {
                    "NODE_ENV": "develop"
                },
                "console": "integratedTerminal",
                "sourceMaps": false,
                "outFiles": []
            }
        ]
    }